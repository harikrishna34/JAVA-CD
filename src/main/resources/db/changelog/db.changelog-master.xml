<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!-- ChangeSet to include schema creation SQL file -->
    <changeSet id="1" author="akash.chavhan">
        <sqlFile path="db.changelog-1.0.sql" relativeToChangelogFile="true"/>
    </changeSet>

    <!-- ChangeSet to include table creation SQL file -->
    <changeSet id="2" author="akash.chavhan">
        <sqlFile path="db.changelog-data-1.0.sql" relativeToChangelogFile="true"/>
    </changeSet>

</databaseChangeLog>
